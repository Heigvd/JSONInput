{
  "name": "jsoninput",
  "version": "0.0.27",
  "description": "Form creation from JSONSchema input",
  "main": "dist/index.js",
  "types": "typings/jsoninput.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "start": "webpack serve --hot --mode development",
    "test": "jest",
    "build": "yarn run clean && jest && yarn run build:webpack"
  },
  "author": "CiGit",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!src/play/**"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/statix/"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "dependencies": {
    "immer": "^9.0.16",
    "jsonschema": "^1.4.1",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "13.4.0",
    "@types/jest": "^29.2.4",
    "@types/lodash-es": "^4.17.6",
    "@types/prop-types": "^15.7.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.14",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "url": "^0.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "17.x"
  }
}
